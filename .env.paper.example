# Alpaca API credentials for Paper Trading
# Copy this file to .env and fill in your paper trading keys.

APCA_API_KEY_ID="YOUR_PAPER_API_KEY"
APCA_API_SECRET_KEY="YOUR_PAPER_SECRET_KEY"

# Set to true for paper trading, false for live
APCA_PAPER_TRADING=true

# API URLs (Optional, defaults are for paper trading if APCA_PAPER_TRADING is true)
APCA_API_BASE_URL="https://paper-api.alpaca.markets/v2"
APCA_API_DATA_URL="https://data.alpaca.markets/v2"
APCA_API_DATA_V1BETA1_URL="https://data.alpaca.markets/v1beta1"
