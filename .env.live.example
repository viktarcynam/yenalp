# Alpaca API credentials for Live Trading
# Copy this file to .env and fill in your live trading keys.

APCA_API_KEY_ID="YOUR_LIVE_API_KEY"
APCA_API_SECRET_KEY="YOUR_LIVE_SECRET_KEY"

# Set to true for paper trading, false for live
APCA_PAPER_TRADING=false

# API URLs (Optional, defaults are for live trading if APCA_PAPER_TRADING is false)
APCA_API_BASE_URL="https://api.alpaca.markets/v2"
APCA_API_DATA_URL="https://data.alpaca.markets/v2"
APCA_API_DATA_V1BETA1_URL="https://data.alpaca.markets/v1beta1"
